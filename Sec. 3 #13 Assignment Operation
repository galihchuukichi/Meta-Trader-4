//+------------------------------------------------------------------+
//| Script program start function                                    |
//+------------------------------------------------------------------+

// Sec. 3 #13 Assignment Operation
void OnStart()
  {
//---
  int x=5,y=10,tmp;
  tmp = x;
  x = y;
  y = tmp;
  
  y += x; // y = y+x;
  x -= y;
  x *= x;
  x /= y;
  y %= x; // y = y%x;
  }
//+------------------------------------------------------------------+
