//| Script program start function                                    |
//+------------------------------------------------------------------+
// Sec. 3 #14 Relational Operations
void OnStart()
  {
//---
   int x=5,y=5;
   x == y;
   y++; x != y;
   x < y; 
   y-=2; x > y;
   y++; x <= y; x >= y;
  }
//+------------------------------------------------------------------+
